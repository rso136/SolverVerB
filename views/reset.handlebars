<div class="ui four column stackable padded centered grid">
	<div class="column">
	{{#each users}}
		<form class="ui form" action="/users/resetpass/{{this.random}}" method="POST">	

				<h4>Enter your new password:</h4>
				<div class="ui huge input">
					<input type="text" id="passInput" name="password">
				</div>	

			<br>
			<br>
			<button type="submit" class="ui huge blue button">Reset Password</button>	
		</form>	
	{{/each}}	
	</div>	
</div>